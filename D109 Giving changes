#include <bits/stdc++.h>
using namespace std;
int main() {
  int a;
  cin >> a;
  for (int i=0; i <= 10; i++){
    if((a-1000) >= 0){
      cout << "1000" <<endl;
      a=a-1000;
    }
  }
  for (int i=0; i <= 10; i++){
    if((a-500) >= 0){
      cout << "500" <<endl;
      a=a-500;
    }
  }
  for (int i=0; i <= 10; i++){
    if((a-100) >= 0){
      cout << "100" <<endl;
      a=a-100;
    }
  }
  for (int i=0; i <= 10; i++){
    if((a-50) >= 0){
      cout << "50" <<endl;
      a=a-50;
    }
  }
  for (int i=0; i <= 10; i++){
    if((a-20) >= 0){
      cout << "20" <<endl;
      a=a-20;
    }
  }
  for (int i=0; i <= 10; i++){
    if ((a-10) >= 0){
      cout << "10" <<endl;
      a=a-10;
    }
  }
}
