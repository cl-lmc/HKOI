#include <bits/stdc++.h>
using namespace std;
int main() {
  int a,z,j=0;
  cin >> a;
  int c[a];
  unordered_map<int,int>x;
  for(int i=0; i<a; i++){
    cin >> z;
    if(x[z] == 0){
      x[z]=1;
      c[j]=z;
      j++;
    }
  }
  cout << j <<endl;
  for(int i=0; i<j; i++){
    cout << c[i] << " ";
  }
  return 0;
}
