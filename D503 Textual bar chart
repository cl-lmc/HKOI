#include <bits/stdc++.h>
using namespace std;
int main() {
  freopen("chart.txt","w",stdout);
  int a,b,c,d,e,f;
  cin >> a >> b >> c >> d >> e >> f;
  cout << "+-------------------------+" <<endl;
  for(int i=10; i>=1; i--){
    cout << "| ";
    if(a>=i){
      cout << "### ";
    }
    else{
      cout << "    ";
    }
    if(b>=i){
      cout << "### ";
    }
    else{
      cout << "    ";
    }
    if(c>=i){
      cout << "### ";
    }
    else{
      cout << "    ";
    }
    if(d>=i){
      cout << "### ";
    }
    else{
      cout << "    ";
    }
    if(e>=i){
      cout << "### ";
    }
    else{
      cout << "    ";
    }
    if(f>=i){
      cout << "### ";
    }
    else{
      cout << "    ";
    }
    cout << "|" <<endl;
  }
  cout << "+-------------------------+";
}
